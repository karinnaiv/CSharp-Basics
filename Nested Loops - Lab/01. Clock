using System;

namespace _01._Clock
{
    internal class Program
    {
        static void Main(string[] args)
        {
            for(int n= 0; n <= 23; n++)
            {
                for(int i= 0; i <=59; i++)
                {
                    Console.WriteLine($"{n}:{i}");
                }
            }
        }
    }
}
