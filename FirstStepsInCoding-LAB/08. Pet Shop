using System;

namespace petshop
{
    class Program
    {
        static void Main(string[] args)
        {
            int dogs = int.Parse(Console.ReadLine());
            int cats = int.Parse(Console.ReadLine());
            double a = dogs * 2.50;
            double b = cats * 4;

            Console.WriteLine(a + b + " lv.");
        }
    }
}
