using System;

namespace projectcreate
{
    class Program
    {
        static void Main(string[] args)
        {
            string name = Console.ReadLine();
            int project = int.Parse(Console.ReadLine());
            int result = project * 3;


            Console.WriteLine($"The architect {name} will need {result} hours to complete {project} project/s.");
        }
    }
}
